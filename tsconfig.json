{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "target": "es2015",
    "module": "es2015",
    "importHelpers": true,
    "baseUrl": ".",
    "removeComments": false,
    "types": [
      "./types/react-native-web",
      "./types/file-loader"
    ],
    // "outDir": "./tsc-out",
    "incremental": true,
    "noEmitOnError": false
  },
  "exclude": [
    "./node_modules"
  ],
  "files": [
    "./src/index.ts"
  ]
}